{
  "compilerOptions": {
    "target": "ES2020",
    "module": "nodenext",
    "lib": [
      "ES2020"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "nodenext",
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "include": ["src/**/*"],

}